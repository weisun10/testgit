<html>
    <head>
        <title>Loading</title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta name="ROBOTS" content="NOINDEX, NOFOLLOW, NOARCHIVE">

        <script type="text/javascript">
            <!--
            function redirectToLoadingPage() {
                var width = -1;
                if(window && window.screen && window.screen.width) {
                    width = window.screen.width;
                }
                if (width > 0 && width <= 500) {
                    location="compactLoading.htm";
                } else {
                    location="loading.htm";
                }
            }
            -->
        </script>
    </head>
    <body onLoad='redirectToLoadingPage();'>
        <p>
            Click&nbsp;<a id='loginPageLink' href='auth/login.aspx'>here</a>&nbsp;if you are not automatically redirected.
        </p>
    </body>
